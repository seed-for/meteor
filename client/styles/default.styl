@require '.includes'
@import 'nib'

@import url('/fonts/NotoSans.css')

html
  box-sizing border-box
  background-color color-background

*,
*:before,
*:after
  box-sizing inherit

body
  background-color color-background
  color color-text
  font-family font-family

a
  color inherit
  text-decoration none
